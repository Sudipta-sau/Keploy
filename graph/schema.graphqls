# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type TestSetStatus {
  status: String!
}

type Query {
  testSets: [String!]!
  testSetStatus(testRunId: String!): TestSetStatus!
}

type BootReplayResponse{
  testRunId: String!
  appId: String!
}

type Mutation {
  bootReplay(): BootReplayResponse!
  runTestSet(testSet: String!): Void
  stopProxy: Boolean!
}
