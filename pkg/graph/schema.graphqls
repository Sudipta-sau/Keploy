
# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type TestRunInfo {
  appId: Int!
  testRunId: String!
}

type TestSetStatus {
  status: String!
}


type Query {
  testSets: [String!]!
  testSetStatus(testRunId: String!, testSetId: String!): TestSetStatus!
  stop: Boolean!
}

type Mutation {
  runTestSet(testSetId: String!, testRunId: String!, appId: Int!): String!
  start: TestRunInfo!
}

